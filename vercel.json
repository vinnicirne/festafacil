{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|webp|map|json|webmanifest))", "dest": "/$1" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}